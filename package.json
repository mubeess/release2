{
  "name": "safsims",
  "version": "1.0.0",
  "assets": [
    "assets/fonts"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "web": "expo start --web",
    "format": "prettier --write src/**/*.{js,ts,jsx,tsx} --no-error-on-unmatched-pattern",
    "format:ci": "prettier --check src/**/*.ts{,x} --no-error-on-unmatched-pattern",
    "format-client": "prettier --write src/generated",
    "lint:ci": "yarn lint --no-error-on-unmatched-pattern --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install && chmod +x .husky/pre-commit",
    "generate-client": "openapi --input https://api.safsims.flexisafapps-dev.com/v2/api-docs --output ./src/generated --useOptions --useUnionTypes --request ./src/api/request.ts --client axios && yarn format-client"
  },
  "dependencies": {
    "@marstankev/react-native-skeleton-content": "^1.0.28",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/checkbox": "^0.5.15",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.27.2",
    "currency-symbol-map": "^5.1.0",
    "expo": "~48.0.18",
    "expo-checkbox": "^2.3.1",
    "expo-image-picker": "~14.1.1",
    "expo-status-bar": "~1.4.0",
    "formik": "^2.2.9",
    "intl": "^1.2.5",
    "locale-currency": "^0.0.2",
    "moment": "^2.29.4",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-chart-kit": "^6.12.0",
    "react-native-charts-wrapper": "^0.5.11",
    "react-native-format-currency": "^0.0.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paystack-webview": "^4.0.3",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-skeleton-content": "^1.0.28",
    "react-native-svg": "13.4.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-ui-lib": "^7.2.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.26.0",
    "react-redux": "^8.0.2",
    "react-select-country-list": "^2.2.3",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "use-debounce": "^9.0.3",
    "yarn": "^1.22.19",
    "yup": "^1.0.2",
    "expo-splash-screen": "~0.18.2"
  },
  "devDependencies": {
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.7.1",
    "typescript": "^4.9.4",
    "@babel/core": "^7.20.0"
  },
  "private": true
}
